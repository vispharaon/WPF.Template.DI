<local:BasePage x:TypeArguments="core:LoginViewModel"  
                x:Class="WPF.Template.DI.Pages.LoginPage"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                xmlns:local="clr-namespace:WPF.Template.DI.Pages"
                xmlns:core="clr-namespace:WPF.Template.DI.Core.ViewModel.Login;assembly=WPF.Template.DI.Core"
                mc:Ignorable="d" 
                d:DesignHeight="450" d:DesignWidth="800"
                >
    <Grid>
        <TextBlock VerticalAlignment="Center"
                   HorizontalAlignment="Center"
                   Text="{Binding TestString}" />
    </Grid>
</local:BasePage>
